<% if @user.superior %>
  <% if @edit_overwork_check.count > 0 %>
     <div class="inline-applicatioins">
       <a id="overtime" data-toggle="modal" data-target="#modal-edit_overwork">
         <%= "【勤怠変更申請のお知らせ】" %>
         <%= "#{@edit_overwork_check.count}件の通知があります" %>
       </a><br>
     </div>
     <%= render 'modal_edit_overwork' %>
<% else %>
    <div id="blue">
      <%= "【勤怠変更申請のお知らせはありません】" %>
    </div>
<% end %>

<% end %>